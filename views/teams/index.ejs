<h2>Search Teams</h2>
<form action="/teams" method="GET">
    <label>Name</label>
    <input type="text" name="name" value="<%= searchOptions.name %>">
    <button type="submit">Search</button>
</form>
<br>
<% teams.forEach(team => { %>
    <div><%= team.name %></div>
    <div><%= team.type %></div>

<% })%>